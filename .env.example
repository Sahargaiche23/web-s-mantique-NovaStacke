# Configuration de l'application EcoTravel

# Flask Configuration
FLASK_APP=backend/app.py
FLASK_ENV=development
FLASK_DEBUG=True

# Server Configuration
HOST=0.0.0.0
PORT=5000

# Ontology Configuration
ONTOLOGY_PATH=ontology/ecotourisme.owl

# API Configuration
API_TIMEOUT=30
MAX_RESULTS=100

# Recommendation Engine
DEFAULT_ECO_WEIGHT=0.6
DEFAULT_BUDGET_WEIGHT=0.3
DEFAULT_PREFERENCE_WEIGHT=0.1

# CORS Configuration
CORS_ORIGINS=http://localhost:3000,http://localhost:5000
