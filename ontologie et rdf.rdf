    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://example.org/ecotourisme#ActivitéTouristique -->

    <Class rdf:about="http://example.org/ecotourisme#ActivitéTouristique"/>
    


    <!-- http://example.org/ecotourisme#Avion -->

    <Class rdf:about="http://example.org/ecotourisme#Avion">
        <rdfs:subClassOf rdf:resource="http://example.org/ecotourisme#Transport"/>
    </Class>
    


    <!-- http://example.org/ecotourisme#CertificationÉcologique -->

    <Class rdf:about="http://example.org/ecotourisme#CertificationÉcologique"/>
    


    <!-- http://example.org/ecotourisme#CommunautéLocale -->

    <Class rdf:about="http://example.org/ecotourisme#CommunautéLocale"/>
    


    <!-- http://example.org/ecotourisme#Covoiturage -->

    <Class rdf:about="http://example.org/ecotourisme#Covoiturage">
        <rdfs:subClassOf rdf:resource="http://example.org/ecotourisme#Transport"/>
    </Class>
    


    <!-- http://example.org/ecotourisme#Destination -->

    <Class rdf:about="http://example.org/ecotourisme#Destination"/>
    


    <!-- http://example.org/ecotourisme#EmpreinteCarbone -->

    <Class rdf:about="http://example.org/ecotourisme#EmpreinteCarbone"/>
    


    <!-- http://example.org/ecotourisme#GastronomieLocale -->

    <Class rdf:about="http://example.org/ecotourisme#GastronomieLocale"/>
    


    <!-- http://example.org/ecotourisme#Hébergement -->

    <Class rdf:about="http://example.org/ecotourisme#Hébergement">
        <rdfs:subClassOf>
            <Restriction>
                <onProperty rdf:resource="http://example.org/ecotourisme#aCertification"/>
                <someValuesFrom rdf:resource="http://example.org/ecotourisme#CertificationÉcologique"/>
            </Restriction>
        </rdfs:subClassOf>
    </Class>
    


    <!-- http://example.org/ecotourisme#RessourceNaturelle -->

    <Class rdf:about="http://example.org/ecotourisme#RessourceNaturelle"/>
    


    <!-- http://example.org/ecotourisme#Train -->

    <Class rdf:about="http://example.org/ecotourisme#Train">
        <rdfs:subClassOf rdf:resource="http://example.org/ecotourisme#Transport"/>
    </Class>
    


    <!-- http://example.org/ecotourisme#Transport -->

    <Class rdf:about="http://example.org/ecotourisme#Transport">
        <rdfs:subClassOf>
            <Restriction>
                <onProperty rdf:resource="http://example.org/ecotourisme#aEmpreinte"/>
                <someValuesFrom rdf:resource="http://example.org/ecotourisme#EmpreinteCarbone"/>
            </Restriction>
        </rdfs:subClassOf>
    </Class>
    


    <!-- http://example.org/ecotourisme#TransportPublic -->

    <Class rdf:about="http://example.org/ecotourisme#TransportPublic">
        <rdfs:subClassOf rdf:resource="http://example.org/ecotourisme#Transport"/>
    </Class>
    


    <!-- http://example.org/ecotourisme#Voyageur -->

    <Class rdf:about="http://example.org/ecotourisme#Voyageur">
        <rdfs:subClassOf>
            <Restriction>
                <onProperty rdf:resource="http://example.org/ecotourisme#choisit"/>
                <someValuesFrom rdf:resource="http://example.org/ecotourisme#Destination"/>
            </Restriction>
        </rdfs:subClassOf>
    </Class>
    


    <!-- http://example.org/ecotourisme#Vélo -->

    <Class rdf:about="http://example.org/ecotourisme#Vélo">
        <rdfs:subClassOf rdf:resource="http://example.org/ecotourisme#Transport"/>
    </Class>
    


    <!-- http://example.org/ecotourisme#ÉvénementCulturel -->

    <Class rdf:about="http://example.org/ecotourisme#ÉvénementCulturel"/>

