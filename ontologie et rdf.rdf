<!-- Voyageur -->
<NamedIndividual rdf:about="http://example.org/ecotourisme#Ali">
  <rdf:type rdf:resource="http://example.org/ecotourisme#Voyageur"/>
  <aBudget rdf:datatype="http://www.w3.org/2001/XMLSchema#decimal">1500.0</aBudget>
  <aPréférence rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Culture et nature</aPréférence>
  <aProfilÉcologique rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Éco-responsable</aProfilÉcologique>
  <choisit rdf:resource="http://example.org/ecotourisme#TunisCarthage"/>
</NamedIndividual>

<!-- Destination -->
<NamedIndividual rdf:about="http://example.org/ecotourisme#TunisCarthage">
  <rdf:type rdf:resource="http://example.org/ecotourisme#Destination"/>
  <aLocalisation rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Tunisie</aLocalisation>
  <aBiodiversité rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Riche flore et faune méditerranéennes</aBiodiversité>
  <propose rdf:resource="http://example.org/ecotourisme#HotelEcoGreen"/>
  <organise rdf:resource="http://example.org/ecotourisme#FestivalMedina"/>
</NamedIndividual>

<!-- Hébergement -->
<NamedIndividual rdf:about="http://example.org/ecotourisme#HotelEcoGreen">
  <rdf:type rdf:resource="http://example.org/ecotourisme#Hébergement"/>
  <aConsommationÉnergie rdf:datatype="http://www.w3.org/2001/XMLSchema#decimal">120.5</aConsommationÉnergie>
  <aCertification rdf:resource="http://example.org/ecotourisme#LabelGreen"/>
</NamedIndividual>

<!-- Certification -->
<NamedIndividual rdf:about="http://example.org/ecotourisme#LabelGreen">
  <rdf:type rdf:resource="http://example.org/ecotourisme#CertificationÉcologique"/>
  <aNiveauCertification rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Gold</aNiveauCertification>
  <aCritèreRespecté rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Gestion eau, énergie renouvelable, recyclage</aCritèreRespecté>
</NamedIndividual>

<!-- Transport -->
<NamedIndividual rdf:about="http://example.org/ecotourisme#TrainExpress">
  <rdf:type rdf:resource="http://example.org/ecotourisme#Train"/>
  <aEmpreinte rdf:resource="http://example.org/ecotourisme#EmpreinteTrain"/>
</NamedIndividual>

<!-- EmpreinteCarbone -->
<NamedIndividual rdf:about="http://example.org/ecotourisme#EmpreinteTrain">
  <rdf:type rdf:resource="http://example.org/ecotourisme#EmpreinteCarbone"/>
  <aCO2 rdf:datatype="http://www.w3.org/2001/XMLSchema#decimal">45.0</aCO2>
  <aRéductionPossible rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Comparé à l’avion, 70% de moins</aRéductionPossible>
</NamedIndividual>

<!-- ActivitéTouristique -->
<NamedIndividual rdf:about="http://example.org/ecotourisme#RandonnéeAtlas">
  <rdf:type rdf:resource="http://example.org/ecotourisme#ActivitéTouristique"/>
  <aAuthenticitéLocale rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Randonnée avec guides locaux berbères</aAuthenticitéLocale>
  <aImpactEnvironnemental rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Faible impact, respect de la faune</aImpactEnvironnemental>
  <implique rdf:resource="http://example.org/ecotourisme#CommunauteBerbere"/>
</NamedIndividual>

<!-- CommunautéLocale -->
<NamedIndividual rdf:about="http://example.org/ecotourisme#CommunauteBerbere">
  <rdf:type rdf:resource="http://example.org/ecotourisme#CommunautéLocale"/>
  <aInitiativeDurable rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Artisanat équitable et accueil touristique</aInitiativeDurable>
</NamedIndividual>

<!-- ÉvénementCulturel -->
<NamedIndividual rdf:about="http://example.org/ecotourisme#FestivalMedina">
  <rdf:type rdf:resource="http://example.org/ecotourisme#ÉvénementCulturel"/>
  <aSaison rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Été</aSaison>
</NamedIndividual>

<!-- GastronomieLocale -->
<NamedIndividual rdf:about="http://example.org/ecotourisme#CuisineTunisienne">
  <rdf:type rdf:resource="http://example.org/ecotourisme#GastronomieLocale"/>
  <aProduitLocal rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean">true</aProduitLocal>
  <utilise rdf:resource="http://example.org/ecotourisme#OliveLocale"/>
</NamedIndividual>

<!-- RessourceNaturelle -->
<NamedIndividual rdf:about="http://example.org/ecotourisme#OliveLocale">
  <rdf:type rdf:resource="http://example.org/ecotourisme#RessourceNaturelle"/>
  <aFragilité rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Risque de sécheresse</aFragilité>
</NamedIndividual>

</rdf:RDF>