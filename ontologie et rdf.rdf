        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    </DatatypeProperty>
    


    <!-- http://example.org/ecotourisme#aAuthenticitéLocale -->

    <DatatypeProperty rdf:about="http://example.org/ecotourisme#aAuthenticitéLocale">
