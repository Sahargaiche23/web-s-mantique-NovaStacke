        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    </DatatypeProperty>
    


    <!-- http://example.org/ecotourisme#aImpactEnvironnemental -->

    <DatatypeProperty rdf:about="http://example.org/ecotourisme#aImpactEnvironnemental">
        <rdfs:domain rdf:resource="http://example.org/ecotourisme#ActivitéTouristique"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
