        <rdfs:range rdf:resource="http://example.org/ecotourisme#EmpreinteCarbone"/>
    </ObjectProperty>
    


    <!-- http://example.org/ecotourisme#choisit -->

    <ObjectProperty rdf:about="http://example.org/ecotourisme#choisit">
