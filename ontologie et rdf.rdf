    </ObjectProperty>
    


    <!-- http://example.org/ecotourisme#propose -->

    <ObjectProperty rdf:about="http://example.org/ecotourisme#propose">
        <rdfs:domain rdf:resource="http://example.org/ecotourisme#Destination"/>
        <rdfs:range rdf:resource="http://example.org/ecotourisme#Hébergement"/>
    </ObjectProperty>
    


    <!-- http://example.org/ecotourisme#utilise -->

    <ObjectProperty rdf:about="http://example.org/ecotourisme#utilise">
