        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#decimal"/>
    </DatatypeProperty>
    


    <!-- http://example.org/ecotourisme#aCO2 -->

    <DatatypeProperty rdf:about="http://example.org/ecotourisme#aCO2">
