    </ObjectProperty>
    


    <!-- http://example.org/ecotourisme#organise -->

    <ObjectProperty rdf:about="http://example.org/ecotourisme#organise">
        <rdfs:domain rdf:resource="http://example.org/ecotourisme#Destination"/>
