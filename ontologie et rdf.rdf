        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    </DatatypeProperty>
    


    <!-- http://example.org/ecotourisme#aLocalisation -->

    <DatatypeProperty rdf:about="http://example.org/ecotourisme#aLocalisation">
        <rdfs:domain rdf:resource="http://example.org/ecotourisme#Destination"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    </DatatypeProperty>
    


    <!-- http://example.org/ecotourisme#aNiveauCertification -->

    <DatatypeProperty rdf:about="http://example.org/ecotourisme#aNiveauCertification">
