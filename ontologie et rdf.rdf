        <rdfs:range rdf:resource="http://example.org/ecotourisme#Destination"/>
    </ObjectProperty>
    


    <!-- http://example.org/ecotourisme#implique -->

    <ObjectProperty rdf:about="http://example.org/ecotourisme#implique">
        <rdfs:domain rdf:resource="http://example.org/ecotourisme#ActivitéTouristique"/>
